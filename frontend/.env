REACT_APP_API_BASE_URL=http://localhost:8000/api
REACT_APP_APP_NAME=Community Connect
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Pagination Settings
REACT_APP_DEFAULT_PAGE_SIZE=12
REACT_APP_MAX_PAGE_SIZE=100

# Map Configuration
REACT_APP_DEFAULT_LAT=28.6139
REACT_APP_DEFAULT_LNG=77.2090
REACT_APP_DEFAULT_ZOOM=10
REACT_APP_MAP_TILE_URL=https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png

# Theme Configuration
REACT_APP_PRIMARY_COLOR=#0ea5e9
REACT_APP_SECONDARY_COLOR=#d946ef

# Feature Toggles
REACT_APP_ENABLE_MAP=true
REACT_APP_ENABLE_REVIEWS=true
REACT_APP_ENABLE_SEARCH=true

# API Configuration
REACT_APP_API_TIMEOUT=10000